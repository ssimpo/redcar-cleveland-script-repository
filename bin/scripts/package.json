{
    "name": "angular-loader",
    "preferGlobal": false,
    "version": "0.0.1",
    "author": "Stephen Simpson <me@simpo.org>",
    "description": "Loader for Angular, which will scan the page for Angular Apps.  If found, Angular is loaded and each app is bootstrapped.  The loader will load-up a profile for each app indicating any library script dependancies to load.",
    "contributors": [{
        "name": "Stephen Simpson",
        "email": "me@simpo.org"
    }],
    "bin": {
		"bowerUpdate": "./.bin/bowerUpdate"
    },
    "scripts": {
        "start": "node ./.bin/bowerUpdate"
    },
    "keywords": [ "angularjs" ],
    "dependencies" : {
		"q": "~1.0.0",
        "mv": "~2.0.0",
        "bower": "~1.2.0",
		"grunt": "~0.4.0",
		"grunt-cli": "~0.1.0",
		"karma": "~0.12.0",
		"grunt-karma": "~0.8.0",
		"phantomjs": "1.9.6-0",
		"karma-phantomjs-launcher": "~0.1.0",
		"karma-ie-launcher": "~0.1.0",
		"grunt-gaze": "~0.1.0",
		"grunt-concurrent": "~0.5.0",
		"karma-firefox-launcher": "~0.1.0",
		"karma-chrome-launcher": "~0.1.0",
		"karma-jasmine": "~0.1.0"
    },
    "license": "MIT",
    "engines": {
        "node": ">=0.10"
    },
	"app": {
		"directories": {
			"unittests": "../../test/unit",
			"testlib": "../../test/lib"
		}
	}
}